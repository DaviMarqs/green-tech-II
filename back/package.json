{
  "name": "my-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migration:create": "npx typeorm migration:create ./src/database/migrations/$npm_config_name",
    "migration:up": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/database/data-source.ts",
    "migration:down": "npx ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/database/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
